{
  "expo": {
    "name": "E-learningApp",
    "slug": "E-learningApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Trendifylogo.jpg",
    "userInterfaceStyle": "light",
    "scheme": "E-learningApp",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/Trendifylogo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.capati10.ElearningAp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Trendifylogo.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.capati10.ElearningApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "76de77f5-e905-4066-979b-19eab4e30be3"
      }
    },
    "plugins": [
      "expo-build-properties"
    ]
  }
}
